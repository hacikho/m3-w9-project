@using Project.Web.Models;
@model TimeCardModel
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>
<form action="@Url.Action("ClockInOut", "TimeCard")" method="get">

    <div>
        <label>Email Address</label>
        @Html.TextBoxFor(m => m.UserName)
    </div>
    
    <div>
        <button>Submit</button>
    </div>
</form>
